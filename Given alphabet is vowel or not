import java.util.*;

class Main{
    

public static void main(String[]args)
{

    Scanner sc=new Scanner(System.in);
    System.out.print("Enter a single alphabet:");
    char  letter=sc.next().toLowerCase().charAt(0);
    
    switch (letter)
    {
    case 'a' :
    
    case 'e' :
        
    case 'i' :
    
    case 'o' :
    
    case 'u' : System.out.println("The given alphabet is vowels");
               break;
    
    default :  System.out.println("The given is not an alphabet"); 

}}}
